<tr class="<%= project.untagged ? 'untagged' : ''%>" id = "objId_<%=project.id%>">
	<td>
  	<%= render :partial => "shared/action",
  	 :locals =>	{
  	 	 	:action => 'edit',
  		 	:id => project.id, 
  			:label => 'Edit', 
  			:title => 'Edit Project',
  			:fmt => 'js'}
		 %>
	</td>
  <td><%= link_to image_tag("delete-item.png"), { :action => "destroy", :id => project.id },
          :data => { :confirm => 'Are you sure?' }, :method => :delete, :class=> :delete %> </td>
          
  <% Project::INDEX_FIELDS.each do |i| %>
  	<td class="<%=i%>"><%= project.send(i)%></td>
  <% end%>
</tr>