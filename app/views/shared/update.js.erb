// generic - takes any object
// Close the edit dialog
if ($('div#editDialog').dialog('isOpen')) {$('div#editDialog').empty().dialog('close');}

// object has changed, rerender object's row
new_row = "<%=escape_javascript( render(:partial => @object.class.name.demodulize.downcase, :locals =>	{ object: @object})) %>"
Ecoport.updateRow("#objId_<%=@object.id%>", new_row);
